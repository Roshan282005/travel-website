{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "mysqlAdminUsername": {
      "value": "azureuser"
    },
    "mysqlAdminPassword": {
      "value": "${MYSQL_ADMIN_PASSWORD}"
    },
    "mysqlSkuName": {
      "value": "Standard_B1ms"
    },
    "appServicePlanSkuName": {
      "value": "B2"
    }
  }
}
